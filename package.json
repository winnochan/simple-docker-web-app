{
  "name": "simple-docker-web-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "shell": "docker run --rm -it winnochan/web-app",
    "build-docker": "if [ ! -d 'node_modules' ]; then npm i; fi; docker build -t winnochan/web-app .",
    "start-docker": "docker run --rm -p 3000:3000 winnochan/web-app",
    "start": "node ./bin/www",
    "clean": "rm -rf ./node_modules"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0"
  }
}
